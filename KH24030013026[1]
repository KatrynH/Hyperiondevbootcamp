<!-- START | Co-certificate Download Modal Script -->
<script>
    $('button.submit-btn').on('click', function (e) {  
        $nps_scale = $(".nps-scale:checked").val();
        
        let certificate_id = document.querySelector('#co-certificate').dataset.certificateId;
        let coCertficateDownloadURL = document.querySelector('#co-certificate').dataset.certificateUrl;

        $('#coCertificateNPSSurveyModal').modal('hide');
        
        $.ajax({
            type: "POST",
            url: "/portfolio/KH24030013026/",
            dataType: 'json',
            data: {
                nps_scale: $nps_scale,
                csrfmiddlewaretoken: $('input[name=csrfmiddlewaretoken]').val(),
                student_no: "KH24030013026",
                certificate_id: certificate_id
            },
            success: function (response) {
                window.location.reload();
            },
            error: function(response) {
                window.location.reload();
            }
        });
        e.preventDefault();
        return window.location = coCertficateDownloadURL
    });
</script>
<!-- END | Co-certificate Download Modal Script -->



